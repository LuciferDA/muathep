                     function vss_login_submit() {
                     
                         var login_email = jQuery("#vss_solo_login_email").val();
                         var login_pass = jQuery("#vss_solo_login_pass").val();
                         var login_otp = jQuery("#vss_solo_login_otp").val();
                         
